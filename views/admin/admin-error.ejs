<!-- Main Content Area -->
<main class="flex-1 p-8 lg:ml-0 transition-all duration-300">
    <div class="bg-white p-6 rounded-lg shadow-md text-center">
        <!-- Error Message -->
        <div class="flex flex-col justify-center items-center">
            <h1 class="text-4xl font-bold text-gray-800 mb-4">Oops! Something Went Wrong</h1>
            <p class="text-lg text-gray-600 mb-6">We are sorry, but something went wrong on our end. Please try again later.</p>
            <!-- Optionally, a Retry Button or Further Action -->
            <button class="bg-red-500 text-white px-6 py-2 rounded-lg font-semibold hover:bg-red-600 transition duration-200">
                Try Again
            </button>
        </div>
    </div>
</main>
</div>
 
